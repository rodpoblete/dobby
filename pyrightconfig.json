{
  "include": [
    "src",
    "tests"
  ],
  "exclude": [
    "**/__pycache__",
    ".venv"
  ],
  "reportMissingImports": true,
  "reportMissingTypeStubs": false,
  "pythonVersion": "3.13",
  "pythonPlatform": "Linux",
  "executionEnvironments": [
    {
      "root": "src",
      "pythonVersion": "3.13",
      "extraPaths": [
        "src"
      ]
    },
    {
      "root": "tests",
      "pythonVersion": "3.13",
      "extraPaths": [
        "src"
      ]
    }
  ]
}
